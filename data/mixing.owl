    <!-- http://www.ease-crc.org/ont/mixing#CircularDivingToInnerMotion -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/mixing#CircularDivingToInnerMotion">
        <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/mixing#MixingMotion"/>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.ease-crc.org/ont/mixing#radius_lower_bound_relative"/>
                        <owl:hasValue rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">0.0</owl:hasValue>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.ease-crc.org/ont/mixing#radius_upper_bound_relative"/>
                        <owl:hasValue rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">0.7</owl:hasValue>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ease-crc.org/ont/mixing#height_increment"/>
                <owl:hasValue rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">0.1</owl:hasValue>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label xml:lang="en">circular diving to inner motion</rdfs:label>
    </owl:Class>
    


