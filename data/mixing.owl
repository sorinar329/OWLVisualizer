    <!-- http://www.ease-crc.org/ont/mixing#WhirlstormMotion -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/mixing#WhirlstormMotion">
        <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/mixing#MixingMotion"/>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.ease-crc.org/ont/mixing#radius_lower_bound_relative"/>
                        <owl:hasValue rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">0.0</owl:hasValue>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.ease-crc.org/ont/mixing#radius_upper_bound_relative"/>
                        <owl:hasValue rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">0.7</owl:hasValue>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:label xml:lang="en">whirlstorm motion</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ease-crc.org/ont/mixing#WhirlstormThenCircularDivingToInner -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/mixing#WhirlstormThenCircularDivingToInner">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.ease-crc.org/ont/mixing#Motion"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.ease-crc.org/ont/mixing#hasMotion"/>
                        <owl:someValuesFrom>
                            <owl:Class>
                                <owl:intersectionOf rdf:parseType="Collection">
                                    <rdf:Description rdf:about="http://www.ease-crc.org/ont/mixing#WhirlstormMotion"/>
                                    <owl:Restriction>
                                        <owl:onProperty rdf:resource="http://www.ease-crc.org/ont/mixing#followedBy"/>
                                        <owl:someValuesFrom rdf:resource="http://www.ease-crc.org/ont/mixing#CircularDivingToInnerMotion"/>
                                    </owl:Restriction>
                                    <owl:Restriction>
                                        <owl:onProperty rdf:resource="http://www.ease-crc.org/ont/mixing#followedBy"/>
                                        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                                        <owl:onClass rdf:resource="http://www.ease-crc.org/ont/mixing#Motion"/>
                                    </owl:Restriction>
                                </owl:intersectionOf>
                            </owl:Class>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/mixing#CompoundMotion"/>
        <rdfs:label rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString">whirlstorm then circular to diving inner</rdfs:label>
    </owl:Class>
    


